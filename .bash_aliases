# bash aliases
alias ll="ls -FGlAhp --color"
alias grep='grep -in --color=always'
alias rm='rm -v'
alias cp='cp -iv'
alias mv='mv -iv'
alias mkdir='mkdir -pv'
alias less='less -c'

alias ..='cd ../'
alias ...='cd ../../'
alias .3='cd ../../../'
alias .4='cd ../../../../'
alias .5='cd ../../../../../'
